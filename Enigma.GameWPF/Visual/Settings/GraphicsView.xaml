<v:View x:TypeArguments="local:GraphicsViewModel" x:Name="view"
    x:Class="Enigma.GameWPF.Visual.Settings.GraphicsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:v ="clr-namespace:Enigma.GameWPF.Visual"
             xmlns:local="clr-namespace:Enigma.GameWPF.Visual.Settings"
             xmlns:local_="clr-namespace:Enigma.GameWPF.Visual.Settings.Graphics"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <local_:ScreenRatiosView ViewModel="{Binding ViewModel.ScreenRatiosViewModel,ElementName=view}"></local_:ScreenRatiosView>
        <local_:FullScreenView ViewModel="{Binding ViewModel.FullScreenViewModel, ElementName=view}"></local_:FullScreenView>
    </StackPanel>
</v:View>