<Page x:Class="Enigma.GameWPF.Visual.GamePage" x:Name="page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Enigma.GameWPF.Visual"
      xmlns:g="clr-namespace:Enigma.GameWPF.Visual.Game"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" Background="White"
      KeyDown="Page_KeyDown" KeyUp="Page_KeyUp" MouseDown="Page_MouseDown" MouseUp="Page_MouseUp" PreviewMouseWheel="Page_PreviewMouseWheel"
      Loaded="Page_Loaded" Unloaded="Page_Unloaded">
    <Grid>
        <g:GameWorldView ViewModel="{Binding GameWorldViewModel,ElementName=page}"></g:GameWorldView>
    </Grid>
</Page>
