<v:View x:TypeArguments="local:FPSViewModel" x:Name="view"
    x:Class="Enigma.GameWPF.Visual.Game.FPSView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:v ="clr-namespace:Enigma.GameWPF.Visual"
             xmlns:local="clr-namespace:Enigma.GameWPF.Visual.Game"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Focusable="False">
    <DockPanel>
        <TextBlock Text="{Binding ViewModel.FPS,ElementName=view}" DockPanel.Dock="Right" HorizontalAlignment="Right"></TextBlock>
        <TextBlock Text="FPS" DockPanel.Dock="Right" HorizontalAlignment="Right"></TextBlock>
    </DockPanel>
</v:View>